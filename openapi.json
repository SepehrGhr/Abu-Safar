{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Simple Petstore API",
    "description": "A test API to check GitHub rendering."
  },
  "paths": {
    "/pets": {
      "get": {
        "summary": "List all pets",
        "operationId": "listPets",
        "responses": {
          "200": {
            "description": "An array of pets.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}